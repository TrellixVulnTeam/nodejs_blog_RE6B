    <script>
        $(function () {
            $('[data-toggle="tooltip"]').tooltip()
        })
        const old_password = document.getElementById('old_password');
        const new_password = document.getElementById('new_password');
        const btn_change = document.getElementById('btnchange');
        const comfirm = document.getElementById('comfirm');
        const submitchange = document.getElementById('submitchange');

        btn_change.addEventListener('click', (e) => {
            old_password.removeAttribute('hidden');
            new_password.value = "";
            new_password.removeAttribute('readonly');
            comfirm.removeAttribute('hidden');
            submitchange.removeAttribute('hidden');
        });
    </script>
