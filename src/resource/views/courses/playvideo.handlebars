<div class="container-fluid">
  <div class="row">
    <div class="col-md-9" style="padding: 0;">
      <div class="video__main">
        <iframe
          class="main-videodetail"
          src="https://www.youtube.com/embed/{{idVideo}}"
        ></iframe>
      </div>
    </div>
    <div class="col-md-3" style="padding: 0;">
      <div class="videos__list">
        {{!-- <h4 style="margin-bottom: 25px; text-align: center;">Danh sách video</h4> --}}
        <div class="videos__item">
          {{!-- LIST BÀI HỌC --}}
          <ul>

            {{#each dtCourse}}
              <li data-slug="{{this.id_video}}" class="courses-mp4 {{#if_equal @index 0}}videos__item-played{{/if_equal}}">
                {{sum @index}}.
                {{this.name_video}}
              </li>
            {{/each}}

          </ul>
          {{!-- END LIST BÀI HỌC --}}
        </div>
      </div>
    </div>
  </div>
</div>
<script src="/js/course.js"></script>