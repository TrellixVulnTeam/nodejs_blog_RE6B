<div class="course">
    <div class="container">
        <div class="row">
            <div class="col-md-8">
                <h1 class="course__name">{{course.name}}</h1>
                <p class="course__description">{{course.description}}</p>

                <div class="course__willstudy">
                    <h4>Bạn sẽ học được gì</h4>
                    <ul class="willstudy--list">
                        {{#each studied}}

                        <li class="willstudy__list-items d-flex align-items-center">
                            <svg class="svg-icon icon--check" viewBox="0 0 20 20">
                                <path fill="none" style="fill: red;"
                                    d="M7.629,14.566c0.125,0.125,0.291,0.188,0.456,0.188c0.164,0,0.329-0.062,0.456-0.188l8.219-8.221c0.252-0.252,0.252-0.659,0-0.911c-0.252-0.252-0.659-0.252-0.911,0l-7.764,7.763L4.152,9.267c-0.252-0.251-0.66-0.251-0.911,0c-0.252,0.252-0.252,0.66,0,0.911L7.629,14.566z">
                                </path>
                            </svg>
                            <span>{{this.studied}}</span>
                        </li>

                        {{/each}}
                    </ul>

                </div>

                <div class="course__content">
                    <h4 style="margin: 15px 0;">Nội dung khóa học</h4>
                    <div class="content-videos">
                        <ul class="content__videos-list">
                            {{#each dtcourse}}
                            <li>
                                <a href="/course/video/{{this.slug_course}}/{{id_video}}"
                                    class="d-flex align-items-center">
                                    <i class="fas fa-play-circle"></i>
                                    <span style="margin-left: 10px;">{{sum @index}}. {{this.name_video}}</span>
                                </a>
                            </li>
                            {{/each}}
                        </ul>
                    </div>
                </div>

                <div class="course__request">
                    <h4 class="title">Yêu cầu</h4>
                    <div class="request-contain">
                        <ul class="intro--list">
                            {{#each request }}
                            <li class="requesr-content--items d-flex align-items-center">
                                <svg class="svg-icon icon--check" viewBox="0 0 20 20">
                                    <path fill="none" style="fill: red;"
                                        d="M7.629,14.566c0.125,0.125,0.291,0.188,0.456,0.188c0.164,0,0.329-0.062,0.456-0.188l8.219-8.221c0.252-0.252,0.252-0.659,0-0.911c-0.252-0.252-0.659-0.252-0.911,0l-7.764,7.763L4.152,9.267c-0.252-0.251-0.66-0.251-0.911,0c-0.252,0.252-0.252,0.66,0,0.911L7.629,14.566z">
                                    </path>
                                </svg>
                                <span>{{this.request}}</span>
                            </li>

                            {{/each}}
                        </ul>
                    </div>
                </div>
            </div>
            <div class="col-md-4 d-flex flex-column align-items-center">
                <div class="course__videowellcome">
                    <iframe width="auto" height="auto" src="https://www.youtube.com/embed/yuaiu-r5DrE" frameborder="0"
                        allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
                        allowfullscreen></iframe>
                </div>
                <p>Bạn đã đăng ký khóa học này!</p>
                <button class="btn btn_allCourses">HỌC TIẾP</button>

